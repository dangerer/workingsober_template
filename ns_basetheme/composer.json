{
  "name": "nitsan/ns-basetheme",
  "type": "typo3-cms-extension",
  "description": "The architecture of parent/child TYPO3 theme concept. Explore https://t3terminal.com and https://nitsantech.com",
  "keywords": [
    "TYPO3 CMS",
    "Basetheme",
    "bootstrap",
    "responsive",
    "frontend",
    "theme",
    "template",
    "ns"
  ],
  "homepage": "https://gitlab.com/sanjay_nitsan/ns_basetheme",
  "authors": [
    {
        "name": "Team NITSAN",
        "email": "sanjay@nitsan.in",
        "role": "Developer",
        "homepage": "http://www.nitsan.in/"
    }
  ],
  "support": {
    "issues": "https://gitlab.com/sanjay_nitsan/ns_basetheme/issues"
  },
  "license": "MIT",
  "require": {
    "typo3/cms-core": ">=8.0.0"
  },
  "autoload": {
    "psr-4": {
      "NITSAN\\NsBasetheme\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/ns-basetheme": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
      "typo3/cms": {
          "cms-package-dir": "{$vendor-dir}/typo3/cms",
          "web-dir": ".Build/public",
          "extension-key": "ns_basetheme"
      }
  }
}